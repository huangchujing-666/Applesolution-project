    <div class="home_banner">
  <div class="left_banner" id="leftbanner">
    @foreach (var item in Model.LeftBanners)
    {
        if (item.link != "")
        {
    <div><a href="@item.link" title="@item.title"><img src="@Url.Content("~/materials/banner")/@item.file_path" width="362" height="71" alt="@item.title" border="0" /></a></div>
        }
        else
        {
    <div><img src="@Url.Content("~/materials/banner")/@item.file_path" width="362" height="71" alt="@item.title" border="0" /></div>
        }
    }
  </div>
<script type="text/javascript">
    $(function () {
        $('#leftbanner').cycle({
            fx: 'fade'
        });
    });
</script>

  <div class="right_banner" id="rightbanner">
    @foreach (var item in Model.RightBanners)
    {
        if (item.link != "")
        {
    <div><a href="@item.link" title="@item.title"><img src="@Url.Content("~/materials/banner")/@item.file_path" width="362" height="71" alt="@item.title" border="0" /></a></div>
        }
        else
        {
    <div><img src="@Url.Content("~/materials/banner")/@item.file_path" width="362" height="71" alt="@item.title" border="0" /></div>
        }
    }
  </div>
<script type="text/javascript">
    $(function () {
        $('#rightbanner').cycle({
            fx: 'fade'
        });
    });
</script>
    </div>
<div style="clear:both"></div>
